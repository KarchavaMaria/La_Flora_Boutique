{"ast":null,"code":"import{createSlice}from'@reduxjs/toolkit';const initialState={favorites:JSON.parse(localStorage.getItem('favorites'))||[]};const favoriteSlice=createSlice({name:'favorite',initialState,reducers:{setFavorite:(state,action)=>{state.favorites=action.payload;localStorage.setItem('favorites',JSON.stringify(state.favorites));},addFavorite:(state,action)=>{state.favorites.push(action.payload);localStorage.setItem('favorites',JSON.stringify(state.favorites));},deleteFavorite:(state,action)=>{state.favorites=state.favorites.filter(i=>i.id!==action.payload.id);localStorage.setItem('favorites',JSON.stringify(state.favorites));}}});export const{setFavorite,addFavorite,deleteFavorite}=favoriteSlice.actions;export default favoriteSlice.reducer;","map":{"version":3,"names":["createSlice","initialState","favorites","JSON","parse","localStorage","getItem","favoriteSlice","name","reducers","setFavorite","state","action","payload","setItem","stringify","addFavorite","push","deleteFavorite","filter","i","id","actions","reducer"],"sources":["/Users/test/Desktop/Flowers/my-app/client/src/store/favoriteSlice.js"],"sourcesContent":["import { createSlice } from '@reduxjs/toolkit';\n\nconst initialState = {\n  favorites: JSON.parse(localStorage.getItem('favorites')) || [],\n};\n\nconst favoriteSlice = createSlice({\n  name: 'favorite',\n  initialState,\n  reducers: {\n    setFavorite: (state, action) => {\n      state.favorites = action.payload;\n      localStorage.setItem('favorites', JSON.stringify(state.favorites));\n    },\n    addFavorite: (state, action) => {\n      state.favorites.push(action.payload);\n      localStorage.setItem('favorites', JSON.stringify(state.favorites));\n    },\n    deleteFavorite: (state, action) => {\n      state.favorites = state.favorites.filter(\n        (i) => i.id !== action.payload.id\n      );\n      localStorage.setItem('favorites', JSON.stringify(state.favorites));\n    },\n  },\n});\n\nexport const { setFavorite, addFavorite, deleteFavorite } =\n  favoriteSlice.actions;\nexport default favoriteSlice.reducer;\n"],"mappings":"AAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,YAAY,CAAG,CACnBC,SAAS,CAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,CAAC,EAAI,EAC9D,CAAC,CAED,KAAM,CAAAC,aAAa,CAAGP,WAAW,CAAC,CAChCQ,IAAI,CAAE,UAAU,CAChBP,YAAY,CACZQ,QAAQ,CAAE,CACRC,WAAW,CAAEA,CAACC,KAAK,CAAEC,MAAM,GAAK,CAC9BD,KAAK,CAACT,SAAS,CAAGU,MAAM,CAACC,OAAO,CAChCR,YAAY,CAACS,OAAO,CAAC,WAAW,CAAEX,IAAI,CAACY,SAAS,CAACJ,KAAK,CAACT,SAAS,CAAC,CAAC,CACpE,CAAC,CACDc,WAAW,CAAEA,CAACL,KAAK,CAAEC,MAAM,GAAK,CAC9BD,KAAK,CAACT,SAAS,CAACe,IAAI,CAACL,MAAM,CAACC,OAAO,CAAC,CACpCR,YAAY,CAACS,OAAO,CAAC,WAAW,CAAEX,IAAI,CAACY,SAAS,CAACJ,KAAK,CAACT,SAAS,CAAC,CAAC,CACpE,CAAC,CACDgB,cAAc,CAAEA,CAACP,KAAK,CAAEC,MAAM,GAAK,CACjCD,KAAK,CAACT,SAAS,CAAGS,KAAK,CAACT,SAAS,CAACiB,MAAM,CACrCC,CAAC,EAAKA,CAAC,CAACC,EAAE,GAAKT,MAAM,CAACC,OAAO,CAACQ,EACjC,CAAC,CACDhB,YAAY,CAACS,OAAO,CAAC,WAAW,CAAEX,IAAI,CAACY,SAAS,CAACJ,KAAK,CAACT,SAAS,CAAC,CAAC,CACpE,CACF,CACF,CAAC,CAAC,CAEF,MAAO,MAAM,CAAEQ,WAAW,CAAEM,WAAW,CAAEE,cAAe,CAAC,CACvDX,aAAa,CAACe,OAAO,CACvB,cAAe,CAAAf,aAAa,CAACgB,OAAO","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}