=> {\n      const item = state.items.find((i) => i.id === action.payload);\n      if (!item) return;\n\n      if (item.quantity > 1) {\n        item.quantity -= 1;\n      } else {\n        state.items = state.items.filter((i) => i.id !== action.payload);\n      }\n\n      calculateTotalPrice(state);\n      saveToLocalStorage(state);\n    },\n\n    clearCart: (state) => {\n      state.items = [];\n      calculateTotalPrice(state);\n      saveToLocalStorage(state);\n    },\n  },\n});\n\nexport const { addToCart, decreaseQuantity, removeFromCart, clearCart } =\n  cartSlice.actions;\nexport default cartSlice.reducer;\n"],"mappings":"8HAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,WAAW,CAAGC,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,CACrD,KAAM,CAAAC,YAAY,CAAGH,WAAW,CAAGI,IAAI,CAACC,KAAK,CAACL,WAAW,CAAC,CAAG,EAAE,CAE/D,KAAM,CAAAM,YAAY,CAAG,CACnBC,KAAK,CAAEJ,YAAY,CACnBK,UAAU,CAAEC,MAAM,CAACR,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC,CAAC,EAAI,CAAC,CAC3DQ,UAAU,CAAED,MAAM,CAACR,YAAY,CAACC,OAAO,CAAC,YAAY,CAAC,CAAC,EAAI,CAC5D,CAAC,CAED,KAAM,CAAAS,kBAAkB,CAAIC,KAAK,EAAK,CACpCX,YAAY,CAACY,OAAO,CAAC,WAAW,CAAET,IAAI,CAACU,SAAS,CAACF,KAAK,CAACL,KAAK,CAAC,CAAC,CAC9DN,YAAY,CAACY,OAAO,CAAC,YAAY,CAAET,IAAI,CAACU,SAAS,CAACF,KAAK,CAACJ,UAAU,CAAC,CAAC,CACpEP,YAAY,CAACY,OAAO,CAAC,YAAY,CAAET,IAAI,CAACU,SAAS,CAACF,KAAK,CAACF,UAAU,CAAC,CAAC,CACtE,CAAC,CAED,KAAM,CAAAK,mBAAmB,CAAIH,KAAK,EAAK,CACrCA,KAAK,CAACF,UAAU,CAAGE,KAAK,CAACL,KAAK,CAACS,MAAM,CACnC,CAACC,KAAK,CAAEC,IAAI,GAAKD,KAAK,EAAIC,IAAI,CAACC,QAAQ,EAAI,CAAC,CAAC,CAC7C,CACF,CAAC,CACDP,KAAK,CAACJ,UAAU,CAAGI,KAAK,CAACL,KAAK,CAACS,MAAM,CACnC,CAACC,KAAK,CAAEC,IAAI,GAAKD,KAAK,CAAG,CAACC,IAAI,CAACC,QAAQ,EAAI,CAAC,GAAKD,IAAI,CAACE,UAAU,EAAI,CAAC,CAAC,CACtE,CACF,CAAC,CACH,CAAC,CAED,KAAM,CAAAC,SAAS,CAAGtB,WAAW,CAAC,CAC5BuB,IAAI,CAAE,MAAM,CACZhB,YAAY,CACZiB,QAAQ,CAAE,CACRC,SAAS,CAAEA,CAACZ,KAAK,CAAEa,MAAM,GAAK,CAC5B,KAAM,CAAAP,IAAI,CAAGO,MAAM,CAACC,OAAO,CAC3B,KAAM,CAAAC,QAAQ,CAAGf,KAAK,CAACL,KAAK,CAACqB,IAAI,CAAEC,CAAC,EAAKA,CAAC,CAACC,EAAE,GAAKZ,IAAI,CAACY,EAAE,CAAC,CA